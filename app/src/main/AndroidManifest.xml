<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.navbot">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.dell.navbot.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.dell.navbot.login" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.example.dell.navbot.sing_in" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.example.dell.navbot.profile" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.example.dell.navbot.createpost" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.example.dell.navbot.SettingsActivity" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.example.dell.navbot.cv_woker" android:theme="@style/AppTheme.Dark" />
        <meta-data android:name="android.app.searchable"
            android:resource="@layout/searchable">

        </meta-data>
    </application>

</manifest>